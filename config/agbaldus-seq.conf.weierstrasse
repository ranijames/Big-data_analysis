[DEFAULT]
cores = 8 
max_cores = 30
notify_email = alvarani@gmail.com
genome = /extra/user/bin/bcbionextgen/data/genomes/Hsapiens/GRCh37/seq/GRCh37.fa
transcripts_gtf = /extra/user/bin/bcbionextgen/data/genomes/Hsapiens/GRCh37/rnaseq/ref-transcripts.gtf
transcripts_gtf_mask = /extra/user/bin/bcbionextgen/data/genomes/Hsapiens/GRCh37/rnaseq/ref-transcripts-mask.gtf

[CUFFLINKS]
bin = /extra/user/bin/cufflinks-2.2.1/src/cufflinks

[CUTADAPT]
bin = /home/user/bin/bcbionextgen/data/anaconda/bin/cutadapt
#normal
#adapters = TACACTCTTTCCCTACACGACGCTCTTCCGATCT, GTGACTGGAGTTCAGACGTGTGCTCTTCCGATCT
#rRNA
adapters = AGATGTGTATAAGAGACAG, TCGTCGGCAGCGTCAGATGTGTATAAGAGACAG, CTGTCTCTTATACACATCTGACGCTGCCGACGA, GTCTCGTGGGCTCGGAGATGTGTATAAGAGACAG, CTGTCTCTTATACACATCTCCGAGCCCACGAGAC

[DEFUSE]
cores = 8
config = /home/user/anaconda3/opt/defuse/scripts/config.txt 
data_dir = /extra/user/data/defuse/

[QUALIMAP]
bin = /extra/user/bin/bcbionextgen/tools/bin/qualimap

[RNASEQC]
jar = /extra/user/bin/RNA-SeQC_v1.1.8.jar


[SAMBAMBA]
bin = /extra/user/bin/bcbionextgen/tools/bin/sambamba

[STAR]
bin = STAR
cores = 10
genome_dir = /extra/user/bin/bcbionextgen/data/genomes/Hsapiens/GRCh37/star/
